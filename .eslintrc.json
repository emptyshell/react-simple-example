{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "google",
    "plugin:react/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react", "compat"],
  "rules": {
    "react/prop-types": "off",
    "valid-jsdoc": "off"
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
